      function setting() {

        //business nature
        if (JSON.stringify('<%= model.company.business_nature %>') != '') {
            var businesee_nature = JSON.parse(htmlDecode(JSON.stringify("<%= model.company.business_nature %>")));
            
            var console = window.console || {log:function(){}};
            var rows = businesee_nature.split('$');
            for (var i=1; i<rows.length -1; i++) {
                if (rows[i] != '') {
                    $('input:checkbox[name="business_nature[]"][value=' + rows[i] + ']').prop('checked', true);
                }
            }
        }

        //product group
        if ('<%= JSON.stringify(model.company.product_group) %>' != '') {
            var product_group = JSON.parse(htmlDecode("<%= JSON.stringify(model.company.product_group) %>"));
            var rows = product_group.split('$');
            for (var i=0; i<rows.length; i++) {
                if (rows[i] != '') {
                    $('input:checkbox[name="product_group[]"][value=' + rows[i] + ']').prop('checked', true);
                }
            }
        }

        //certification
        if ('<%= JSON.stringify(model.company.certification) %>' != '') {
            var certification = JSON.parse(htmlDecode("<%= JSON.stringify(model.company.certification) %>"));
            var rows = certification.split('$');
            for (var i=0; i<rows.length; i++) {
                if (rows[i] != '') {
                    $('input:checkbox[name="certification[]"][value=' + rows[i] + ']').prop('checked', true);
                }
            }
        }

        //category
        if (categoryList.length > 0) {
            for (var i=0; i<categoryList.length; i++) {
                var obj = { 'id': categoryList[i].id };
                if (categoryList[i].depth == 4) {
                    $('#categoryTags').tagsinput('add', categoryList[i].p1_name + ' > ' + categoryList[i].p2_name + ' > ' + categoryList[i].p3_name + ' > ' + categoryList[i].category_name);
                    categoryMap[categoryList[i].p1_name + ' > ' + categoryList[i].p2_name + ' > ' + categoryList[i].p3_name + ' > ' + categoryList[i].category_name] = obj;
                }
                if (categoryList[i].depth == 3) {
                    $('#categoryTags').tagsinput('add', categoryList[i].p2_name + ' > ' + categoryList[i].p3_name + ' > ' + categoryList[i].category_name);
                    categoryMap[categoryList[i].p2_name + ' > ' + categoryList[i].p3_name + ' > ' + categoryList[i].category_name] = obj;
                } 
                if (categoryList[i].depth == 2) {
                    $('#categoryTags').tagsinput('add', categoryList[i].p3_name + ' > ' + categoryList[i].category_name);
                    categoryMap[categoryList[i].p3_name + ' > ' + categoryList[i].category_name] = obj;
                } 
                if (categoryList[i].depth == 1) {
                    $('#categoryTags').tagsinput('add', categoryList[i].category_name);
                    categoryMap[categoryList[i].category_name] = obj;
                } 
            }
        }

        //set sub data
        for (key in subMap) {
            $("#" + subMap[key].field_code).val(subMap[key].value_);
        }

        //credit
        var creditRating = JSON.parse(htmlDecode("<%= JSON.stringify(model.company.creditRating) %>"));
        if (creditRating != '' && creditRating != undefined) {
            $('input:radio[name="creditRating"][value=' + creditRating + ']').prop('checked', true);
        } else {
            $('input:radio[name="creditRating"][value=""]').prop('checked', true);
        }

        var cashFlowRating = JSON.parse(htmlDecode("<%= JSON.stringify(model.company.cashFlowRating) %>"));
        if (cashFlowRating != '' && cashFlowRating != undefined) {
            $('input:radio[name="cashFlowRating"][value=' + cashFlowRating + ']').prop('checked', true);
        } else {
            $('input:radio[name="cashFlowRating"][value=""]').prop('checked', true);
        }

        var watchRating = JSON.parse(htmlDecode("<%= JSON.stringify(model.company.watchRating) %>"));
        if (watchRating != '' && watchRating != undefined) {
            $('input:radio[name="watchRating"][value=\"' + watchRating + '\"]').prop('checked', true);
        } else {
            $('input:radio[name="watchRating"][value=""]').prop('checked', true);
        }

        //opinion
        if ('<%= JSON.stringify(model.company.opinion_eng) %>' !=  '') {
            var str = '<%= JSON.stringify(model.company.opinion_eng) %>';
            str = str.replace('&#34;', '').replace("<br>", "\r\n").replace('&amp;', '&').replace('&gt;', '>') .replace('&lt;', '<') .replace('&quot;', '\"') .replace('&apos;', '\'');
            str = replaceAll(str, '&#34;', '\"');
            str = replaceAll(str, "&lt;br&gt;", "\r\n");
            str = replaceAll(str, "<br>", "\r\n");
            str = str.substr(0, str.length -1);
            $('#opinion_eng').val(str);
        }

    }

    //Get the button
    var mybutton = document.getElementById("myBtn");

    // When the user scrolls down 20px from the top of the document, show the button
    window.onscroll = function() {scrollFunction()};

    function scrollFunction() {
    if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
        mybutton.style.display = "block";
    } else {
        mybutton.style.display = "none";
    }
    }

    // When the user clicks on the button, scroll to the top of the document
    function topFunction() {
    document.body.scrollTop = 0;
    document.documentElement.scrollTop = 0;
    }

    function del(id) {

        if (confirm('삭제하시겠습니까?')) {

            var data = { 'tableName': 'company' };
            data = JSON.stringify(data);

            //content-type을 설정하고 데이터 송신
            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/admin/del/' + id);
            xhr.setRequestHeader('Content-type', "application/json");
            xhr.send(data);

            // 데이터 수신이 완료되면 표시
            xhr.addEventListener('load', function(){
                alert("삭제되었습니다.")
                window.location.href = "/admin/company/before?pageNum=" + getParameterByName("pageNum") + "&search_type=" + getParameterByName("search_type") + "&country_id=" + getParameterByName("country_id") 
                    + "&search_txt=" + getParameterByName("search_txt") + "&search_bn=" + getParameterByName("search_bn") + "&search_pg=" + getParameterByName("search_pg") 
                    + "&search_cate=" + getParameterByName("search_cate") + "&categoryText=" + getParameterByName("categoryText");  
            });
        } 
    }

