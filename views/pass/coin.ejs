<script>

    var oldVal;
    var content = "";

    $("document").ready(function() {

      if ('<%= result %>' == 1) {
        alert("Transaction Complete!");
        window.location.href = '/point/deposit';
      }

    });

    //로그인 체크부터
  function check() {

    $.ajax({
          type: "POST",
          url: "/user/loginCheck",
          dataType: "json",
          data: {url: '/pass/coin'},
          success: function (data) {
              if(data.result) {
                payment();
              } else {
                if (confirm('Sign in is required. You will be moved to sign in page.')) {
                  window.location.href = data.fullUrl;
                }
              }
          },
          error: function (e) {
              alert(e.responseJSON.msg)
          }
      });

  }

  //결제창 열기
  function payment() {

    var frm = document.form;
    var url    = "/pass/selMethod_coin" ;
    var title  = "AUTOINPEC" ;
    var status = "width = 710, height = 800, top = 100, left = 200, location = no" ;
    window.open("", title,status); 
                                              
    frm.target = title;                   
    frm.action = url;                    
    frm.method = "post";
    frm.submit();  
  }

</script>
 
